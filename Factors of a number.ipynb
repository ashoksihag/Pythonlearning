    "#find all the factors of a number \n",
    "\n",
    "\n",
    "def factor(n):\n",
    "    for i in range(0,n):\n",
    "        if n%(i+1) == 0:\n",
    "            print(i+1)\n",
    "\n",
    "x = input('Input number : ')\n",
    "\n",
    "y = int(x)\n",
    "\n",
    "print('factors of number ' + x + ' are :')\n",
    "\n",
    "factor(y)\n"
   ]
